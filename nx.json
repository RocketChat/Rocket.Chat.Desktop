{
  "npmScope": "rocket.chat.desktop",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "electron-desktop": {
      "tags": []
    },
    "electron-store": {
      "tags": []
    },
    "ipc-state": {
      "tags": []
    },
    "ui-desktop": {
      "tags": []
    },
    "ui-desktop-components": {
      "tags": []
    },
    "ui-desktop-components-e2e": {
      "tags": [],
      "implicitDependencies": ["ui-desktop-components"]
    },
    "ui-desktop-e2e": {
      "tags": [],
      "implicitDependencies": ["ui-desktop"]
    }
  }
}
